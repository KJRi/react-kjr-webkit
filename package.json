{
  "name": "react-kjr-webkit",
  "version": "1.0.0",
  "description": "webpack demo",
  "main": "index.js",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack",
    "server": "webpack-dev-server --config ./webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.pro.config.js",
    "dev:dll": "webpack --config ./webpack.dll.config.js",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run flow && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "keywords": [],
  "author": "kjr",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.1.1",
    "glob-all": "^3.2.1",
    "html-webpack-plugin": "^4.5.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^1.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "add-asset-html-webpack-plugin": "^3.1.3",
    "better-npm-run": "^0.1.1",
    "cross-env": "^7.0.2",
    "ip": "^1.1.5",
    "purgecss-webpack-plugin": "^3.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "ts-loader": "^8.0.6",
    "webpack-merge": "^5.2.0",
    "yargs": "^16.1.0"
  }
}
